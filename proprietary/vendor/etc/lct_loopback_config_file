tc 1
#Headset-mic to Speaker Channel-L AFE loopback
!AfeLoop
Txdevice:11
Rxdevice:5

enable
ADC2 Volume:1
DEC1 MUX:ADC2
ADC2 MUX:INP2
IIR1 INP1 MUX:DEC1

RX1 MIX1 INP1:RX1
RX1 Digital Volume:77
SPK_REC Switch:SPKL
HPHL:Switch

PRI_MI2S_RX Port Mixer TERT_MI2S_TX:1

disable
PRI_MI2S_RX Port Mixer TERT_MI2S_TX:0

ADC2 Volume:1
DEC1 MUX:ZERO
ADC2 MUX:ZERO
IIR1 INP1 MUX:ZERO

RX1 MIX1 INP1:ZERO
RX1 Digital Volume:84
SPK_REC Switch:Off
HPHL:ZERO




tc 2
#Headset-mic to Speaker Channel-R AFE loopback
!AfeLoop
Txdevice:11
Rxdevice:5

enable
