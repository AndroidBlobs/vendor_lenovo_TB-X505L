tc 4
#Handset-mic to Headset Dual-Channel AFE loopback
!AfeLoop
Txdevice:11
Rxdevice:5

enable
IIR1 INP1 MUX:DEC1
ADC1 Volume:6
DEC1 MUX:ADC1
ADC1_INP1 Switch:1

MI2S_RX Channels:Two
RX1 MIX1 INP1:RX1
RX2 MIX1 INP1:RX2
RDAC2 MUX:RX2
HPHL:Switch
HPHR:Switch

PRI_MI2S_RX Port Mixer TERT_MI2S_TX:1

disable
PRI_MI2S_RX Port Mixer TERT_MI2S_TX:0

ADC1_INP1 Switch:0
DEC1 MUX:ZERO
ADC1 Volume:4
IIR1 INP1 MUX:ZERO

MI2S_RX Channels:One
RX1 MIX1 INP1:ZERO
RX2 MIX1 INP1:ZERO
RDAC2 MUX:ZERO
HPHL:ZERO
HPHR:ZERO
